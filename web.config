    <rewrite>
            <rules>
             <rule name="Rewrite">
              <match url="^blog/$" />
              <action type="Rewrite" url="http://valiat-blog.azurewebsites.net" />
              </rule>
            </rules>
        </rewrite>
  </system.webServer>  
</configuration>